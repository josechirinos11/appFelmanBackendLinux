# Configuración de Base de Datos Felman
# Copia este archivo como .env y completa las variables

# Configuración MySQL/MariaDB
DB_HOST=localhost
DB_PORT=3306
DB_USER=tu_usuario
DB_PASS=tu_contraseña
DB_NAME=felman

# Configuración AI21
AI21_API_KEY=tu_api_key_de_ai21

# Puerto del servidor
PORT=3000

# Ejemplo para conexión local:
# DB_HOST=localhost
# DB_PORT=3306
# DB_USER=root
# DB_PASS=password
# DB_NAME=felman

# Ejemplo para conexión remota:
# DB_HOST=192.168.1.100
# DB_PORT=3306
# DB_USER=felman_user
# DB_PASS=secure_password
# DB_NAME=felman_database
